<?xml version='1.0' encoding='UTF-8'?>
<vortrac xmnls="http://www.eol.ucar.edu/ns/vortrac" >
    <vortex>
        <name>Analytic Storm</name>
        <lat>.9</lat>
        <lon>.9</lon>
        <direction>0</direction>
        <speed>0</speed>
        <obsdate>1900-01-01</obsdate>
        <obstime>00:00:00</obstime>
        <dir>workingDirs/</dir>
    </vortex>
    <radar>
        <name>Plea</name>
        <lat>0.6</lat>
        <lon>0.6</lon>
        <alt>0.00</alt>
        <dir>/scr/science40/mauger/Working/trunk/vortrac_defaultAnalyticTC.xml</dir>
        <format>MODEL</format>
        <startdate>2000-01-01</startdate>
        <enddate>2000-01-02</enddate>
        <starttime>00:00:00</starttime>
        <endtime>00:00:00</endtime>
    </radar>
    <cappi>
        <dir>/scr/science40/mauger/Working/trunk/workingDirs/cappi</dir>
        <xdim>100</xdim>
        <ydim>100</ydim>
        <zdim>3</zdim>
        <xgridsp>1.0</xgridsp>
        <ygridsp>1.0</ygridsp>
        <zgridsp>1.0</zgridsp>
        <adv_u>0.0</adv_u>
        <adv_v>0.0</adv_v>
        <interpolation>cressman</interpolation>
    </cappi>
    <center>
        <dir>/scr/science40/mauger/Working/trunk/workingDirs/center</dir>
        <geometry>GBVTD</geometry>
        <closure>original</closure>
        <reflectivity>DZ</reflectivity>
        <velocity>VE</velocity>
        <bottomlevel>1</bottomlevel>
        <toplevel>3</toplevel>
        <innerradius>5</innerradius>
        <outerradius>20</outerradius>
        <ringwidth>1.0</ringwidth>
        <search>MAXVT0</search>
        <influenceradius>4.0</influenceradius>
        <convergence>0.05</convergence>
        <maxiterations>60</maxiterations>
        <boxdiameter>12.0</boxdiameter>
        <numpoints>16</numpoints>
        <maxwavenumber>1</maxwavenumber>
        <maxdatagap wavenum="0" >180</maxdatagap>
        <maxdatagap wavenum="1" >120</maxdatagap>
    </center>
    <choosecenter>
        <dir>/scr/science40/mauger/Working/trunk/workingDirs/choosecenter</dir>
        <filename>nameOfCenterListFile</filename>
        <startdate>2005-01-01</startdate>
        <enddate>2005-01-02</enddate>
        <starttime>00:00:00</starttime>
        <endtime>00:00:00</endtime>
        <min_volumes>6</min_volumes>
        <dist_weight>0.0</dist_weight>
        <wind_weight>.2</wind_weight>
        <stddev_weight>.6</stddev_weight>
        <pts_weight>.2</pts_weight>
        <position_weight>.4</position_weight>
        <rmw_weight>.4</rmw_weight>
        <vt_weight>.2</vt_weight>
        <stats>99</stats>
    </choosecenter>
    <vtd>
        <dir>/scr/science40/mauger/Working/trunk/workingDirs/vtd</dir>
        <geometry>GBVTD</geometry>
        <closure>original</closure>
        <reflectivity>DZ</reflectivity>
        <velocity>VE</velocity>
        <bottomlevel>1</bottomlevel>
        <toplevel>3</toplevel>
        <innerradius>1</innerradius>
        <outerradius>60</outerradius>
        <ringwidth>1.0</ringwidth>
        <maxwavenumber>1</maxwavenumber>
        <maxdatagap wavenum="0" >180</maxdatagap>
        <maxdatagap wavenum="1" >120</maxdatagap>
    </vtd>
    <hvvp/>
    <pressure>
        <dir>/scr/science40/mauger/Working/trunk/workingDirs/</dir>
        <format>AWIPS</format>
        <maxobstime>59</maxobstime>
        <maxobsdist>50</maxobsdist>
        <maxobsmethod>ring</maxobsmethod>
        <av_interval>8</av_interval>
        <rapidlimit>3</rapidlimit>
    </pressure>
    <graphics>
        <autolimits>on</autolimits>
        <pressmin>900.0</pressmin>
        <pressmax>1013.0</pressmax>
        <rmwmin>0.0</rmwmin>
        <rmwmax>60.0</rmwmax>
        <startdate>2005-01-01</startdate>
        <enddate>2005-01-02</enddate>
        <starttime>00:00:00</starttime>
        <endtime>00:00:00</endtime>
    </graphics>
    <qc>
        <wind_method>vad</wind_method>
        <vel_min>1.0</vel_min>
        <vel_max>100</vel_max>
        <ref_min>-15</ref_min>
        <ref_max>85</ref_max>
        <sw_threshold>30.0</sw_threshold>
        <bbcount>20</bbcount>
        <maxfold>4</maxfold>
        <windspeed>0</windspeed>
        <winddirection>0</winddirection>
        <awips_dir>/path/to/awips/data</awips_dir>
        <gvad>true</gvad>
        <vadlevels>6</vadlevels>
        <numcoeff>3</numcoeff>
        <vadthr>30</vadthr>
        <gvadthr>180</gvadthr>
    </qc>
</vortrac>
