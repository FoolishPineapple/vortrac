<?xml version='1.0' encoding='UTF-8'?>
<vortrac xmnls="http://www.eol.ucar.edu/ns/vortrac" >
    <vortex>
        <name>Analytic Storm</name>
        <lat>.90</lat>
        <lon>.90</lon>
        <direction>35</direction>
        <speed>10</speed>
