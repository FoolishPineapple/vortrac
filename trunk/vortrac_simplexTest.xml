    <vtd>
        <dir>/path/to/vtd/output</dir>
        <geometry>GBVTD</geometry>
        <closure>original</closure>
        <reflectivity>DZ</reflectivity>
        <velocity>VE</velocity>
        <bottomlevel>1</bottomlevel>
        <toplevel>5</toplevel>
