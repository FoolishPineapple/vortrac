    </vortex>
    <radar>
        <name>Plea</name>
        <lat>0.00</lat>
        <lon>0</lon>
        <alt>0.00</alt>
        <dir>/export/poseidon1/mbell/code/vortrac/trunk/vortrac_simplexTestAnalytic.xml</dir>
